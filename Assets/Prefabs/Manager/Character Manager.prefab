    MoodIndex: 20
    slideImage: {fileID: 21300000, guid: 67d3424f85a88402eaeedebfc678591b, type: 3}
    showPrev: 0
    showNext: 1
    showNextScene: 0
    hideAtStart: 0
  - script: The engine is next. You will need the cards shown above.
    MoodIndex: 0
